{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "ADFTutorialDataFactory0210"
		},
		"AzureStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureStorage'"
		},
		"AzureStorageLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureStorageLinkedService'"
		},
		"DestinationDataset_pdo_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "output.txt"
		},
		"DestinationDataset_pdo_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "adftutorial/output"
		},
		"SourceDataset_pdo_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "emp.txt"
		},
		"SourceDataset_pdo_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "adftutorial/input"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureStorage_connectionString": {
						"value": "[parameters('AzureStorage_connectionString')]"
					},
					"AzureStorageLinkedService_connectionString": {
						"value": "[parameters('AzureStorageLinkedService_connectionString')]"
					},
					"DestinationDataset_pdo_properties_typeProperties_fileName": {
						"value": "[parameters('DestinationDataset_pdo_properties_typeProperties_fileName')]"
					},
					"DestinationDataset_pdo_properties_typeProperties_folderPath": {
						"value": "[parameters('DestinationDataset_pdo_properties_typeProperties_folderPath')]"
					},
					"SourceDataset_pdo_properties_typeProperties_fileName": {
						"value": "[parameters('SourceDataset_pdo_properties_typeProperties_fileName')]"
					},
					"SourceDataset_pdo_properties_typeProperties_folderPath": {
						"value": "[parameters('SourceDataset_pdo_properties_typeProperties_folderPath')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}